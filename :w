import db from "../database";
import { and, eq } from "drizzle-orm";

import { posts } from "../database/schema";
const sub = db.$with("sq").as(
  db
    .select()
    .from(posts)
    .where(and(eq(posts.id, ""), eq(posts.userId, ""))),
);

